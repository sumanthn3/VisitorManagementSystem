<core:FragmentDefinition xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="Success" draggable="true" verticalScrolling="true" class="sapUiSizeCompact AddDialog" resizable="true"
		contentWidth="20rem" titleAlignment="Center">
		<content>
		  <Image height="100%" width="100%" src="./image/Success.gif"></Image>
		  <Text text="Thank You" class="exitSuccess" textAlign="Center"></Text>
