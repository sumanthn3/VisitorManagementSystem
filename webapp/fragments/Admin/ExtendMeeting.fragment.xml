<core:FragmentDefinition xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="{i18n>extendMeet}" draggable="true" verticalScrolling="true" class="sapUiSizeCompact AddDialog" resizable="true"
		contentWidth="20rem" titleAlignment="Center">
		<content>
			<VBox>
				<f:SimpleForm layout="ColumnLayout" editable="true">
					<f:content>
						<Label text="Time (in Minutes)"></Label>
				     	<Input type="Number" id="idADExtendTime" ></Input>
					</f:content>
				</f:SimpleForm>
			</VBox>
		</content>
		<beginButton>
			<Button text="{i18n>extend}" type="Success" press="onExtendTime"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" type="Reject" press="onRemoveExtend"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>